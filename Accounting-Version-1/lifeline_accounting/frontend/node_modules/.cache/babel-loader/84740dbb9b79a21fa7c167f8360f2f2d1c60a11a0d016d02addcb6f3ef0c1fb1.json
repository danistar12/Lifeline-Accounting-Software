{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      employees: [],\n      newEmployee: {\n        first_name: '',\n        last_name: '',\n        email: '',\n        phone_number: '',\n        hire_date: ''\n      }\n    };\n  },\n  methods: {\n    fetchEmployees() {\n      axios.get('/api/payroll/employees/').then(response => {\n        this.employees = response.data;\n      }).catch(error => {\n        console.error('Error fetching employees:', error);\n      });\n    },\n    addEmployee() {\n      axios.post('/api/payroll/employees/', this.newEmployee).then(response => {\n        this.employees.push(response.data);\n        this.newEmployee = {\n          first_name: '',\n          last_name: '',\n          email: '',\n          phone_number: '',\n          hire_date: ''\n        };\n      }).catch(error => {\n        console.error('Error adding employee:', error);\n      });\n    },\n    editEmployee(employee) {\n      // Implementation for editing an employee\n    },\n    deleteEmployee(employeeId) {\n      axios.delete(`/api/payroll/employees/${employeeId}/`).then(() => {\n        this.employees = this.employees.filter(employee => employee.employee_id !== employeeId);\n      }).catch(error => {\n        console.error('Error deleting employee:', error);\n      });\n    }\n  },\n  mounted() {\n    this.fetchEmployees();\n  }\n};","map":{"version":3,"names":["axios","data","employees","newEmployee","first_name","last_name","email","phone_number","hire_date","methods","fetchEmployees","get","then","response","catch","error","console","addEmployee","post","push","editEmployee","employee","deleteEmployee","employeeId","delete","filter","employee_id","mounted"],"sources":["C:\\Users\\ds3297l\\Desktop\\Accountint Software\\Design 2\\lifeline_accounting\\frontend\\src\\views\\payroll\\Employees.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>Employees</h1>\r\n    <!-- Add employee form -->\r\n    <form @submit.prevent=\"addEmployee\">\r\n      <input v-model=\"newEmployee.first_name\" placeholder=\"First Name\" required>\r\n      <input v-model=\"newEmployee.last_name\" placeholder=\"Last Name\" required>\r\n      <input v-model=\"newEmployee.email\" placeholder=\"Email\" required>\r\n      <input v-model=\"newEmployee.phone_number\" placeholder=\"Phone Number\">\r\n      <input v-model=\"newEmployee.hire_date\" type=\"date\" required>\r\n      <button type=\"submit\">Add Employee</button>\r\n    </form>\r\n\r\n    <!-- Employees table -->\r\n    <table>\r\n      <thead>\r\n        <tr>\r\n          <th>First Name</th>\r\n          <th>Last Name</th>\r\n          <th>Email</th>\r\n          <th>Phone Number</th>\r\n          <th>Hire Date</th>\r\n          <th>Actions</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"employee in employees\" :key=\"employee.employee_id\">\r\n          <td>{{ employee.first_name }}</td>\r\n          <td>{{ employee.last_name }}</td>\r\n          <td>{{ employee.email }}</td>\r\n          <td>{{ employee.phone_number }}</td>\r\n          <td>{{ employee.hire_date }}</td>\r\n          <td>\r\n            <button @click=\"editEmployee(employee)\">Edit</button>\r\n            <button @click=\"deleteEmployee(employee.employee_id)\">Delete</button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      employees: [],\r\n      newEmployee: {\r\n        first_name: '',\r\n        last_name: '',\r\n        email: '',\r\n        phone_number: '',\r\n        hire_date: ''\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    fetchEmployees() {\r\n      axios.get('/api/payroll/employees/')\r\n        .then(response => {\r\n          this.employees = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching employees:', error);\r\n        });\r\n    },\r\n    addEmployee() {\r\n      axios.post('/api/payroll/employees/', this.newEmployee)\r\n        .then(response => {\r\n          this.employees.push(response.data);\r\n          this.newEmployee = { first_name: '', last_name: '', email: '', phone_number: '', hire_date: '' };\r\n        })\r\n        .catch(error => {\r\n          console.error('Error adding employee:', error);\r\n        });\r\n    },\r\n    editEmployee(employee) {\r\n      // Implementation for editing an employee\r\n    },\r\n    deleteEmployee(employeeId) {\r\n      axios.delete(`/api/payroll/employees/${employeeId}/`)\r\n        .then(() => {\r\n          this.employees = this.employees.filter(employee => employee.employee_id !== employeeId);\r\n        })\r\n        .catch(error => {\r\n          console.error('Error deleting employee:', error);\r\n        });\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchEmployees();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\nth, td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n}\r\nth {\r\n  background-color: #f2f2f2;\r\n}\r\n</style>\r\n"],"mappings":"AA2CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,WAAW,EAAE;QACXC,UAAU,EAAE,EAAE;QACdC,SAAS,EAAE,EAAE;QACbC,KAAK,EAAE,EAAE;QACTC,YAAY,EAAE,EAAE;QAChBC,SAAS,EAAE;MACb;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAAA,EAAG;MACfV,KAAK,CAACW,GAAG,CAAC,yBAAyB,EAChCC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACX,SAAQ,GAAIW,QAAQ,CAACZ,IAAI;MAChC,CAAC,EACAa,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CAAC;MACnD,CAAC,CAAC;IACN,CAAC;IACDE,WAAWA,CAAA,EAAG;MACZjB,KAAK,CAACkB,IAAI,CAAC,yBAAyB,EAAE,IAAI,CAACf,WAAW,EACnDS,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACX,SAAS,CAACiB,IAAI,CAACN,QAAQ,CAACZ,IAAI,CAAC;QAClC,IAAI,CAACE,WAAU,GAAI;UAAEC,UAAU,EAAE,EAAE;UAAEC,SAAS,EAAE,EAAE;UAAEC,KAAK,EAAE,EAAE;UAAEC,YAAY,EAAE,EAAE;UAAEC,SAAS,EAAE;QAAG,CAAC;MAClG,CAAC,EACAM,KAAK,