er';\n      }\n    },\n    userInitials: function userInitials() {\n      if (!this.user) return 'U';\n      var userData = this.user.user || this.user;\n      if (userData.first_name && userData.last_name) {\n        return \"\".concat(userData.first_name.charAt(0)).concat(userData.last_name.charAt(0)).toUpperCase();\n      } else if (userData.first_name) {\n        return userData.first_name.charAt(0).toUpperCase();\n      } else if (userData.username) {\n        return userData.username.charAt(0).toUpperCase();\n      } else {\n        return 'U';\n      }\n    }\n  }),\n  methods: _objectSpread(_objectSpread({}, mapActions(['me'])), {}, {\n    loadProfileData: function loadProfileData() {\n      var _this = this;\n      return _asyncToGenerator(/*#__PURE__*/_regenerator().m(function _callee() {\n        var response, userData, _t;\n        return _regenerator().w(function (_context) {\n          while (1) switch (_context.n) {\n            case 0:\n              _context.p = 0;\n              _context.n = 1;\n              return axios.get('/api/accounts/auth/user/');\n            case 1:\n              response = _context.v;\n              userData = response.data.user;\n              _this.profileForm = {\n                username: userData.username || '',\n                first_name: userData.first_name || '',\n                last_name: userData.last_name || '',\n                email: userData.email || '',\n                user_notes: userData.user_notes || ''\n              };\n\n              // Store original form data for cancel functionality\n              _this.originalForm = _objectSpread({}, _this.profileForm);\n\n              // Load company roles\n              _this.companyRoles = response.data.company_roles || [];\n              _context.n = 3;\n              break;\n            case 2:\n              _context.p = 2;\n              _t = _context.v;\n              console.error('Failed to load profile data:', _t);\n            case 3:\n         