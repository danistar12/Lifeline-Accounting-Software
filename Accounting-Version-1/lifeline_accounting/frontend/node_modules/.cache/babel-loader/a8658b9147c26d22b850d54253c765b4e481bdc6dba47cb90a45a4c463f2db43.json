{"ast":null,"code":"import { mapState, mapActions } from 'vuex';\nimport CompanySelector from './components/CompanySelector.vue';\nexport default {\n  name: 'App',\n  components: {\n    CompanySelector\n  },\n  computed: {\n    ...mapState({\n      isLoggedIn: state => !!state.user.token\n    })\n  },\n  methods: {\n    ...mapActions({\n      logoutAction: 'user/logout'\n    }),\n    login() {\n      this.$router.push({\n        name: 'Login'\n      });\n    },\n    logout() {\n      this.logoutAction().then(() => {\n        this.$router.push({\n          name: 'Login'\n        });\n      });\n    }\n  }\n};","map":{"version":3,"names":["mapState","mapActions","CompanySelector","name","components","computed","isLoggedIn","state","user","token","methods","logoutAction","login","$router","push","logout","then"],"sources":["C:\\Users\\ds3297l\\Desktop\\Accountint Software\\Design 2\\lifeline_accounting\\frontend\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <header class=\"app-header\">\r\n      <div class=\"logo\">\r\n        <router-link to=\"/\">Lifeline</router-link>\r\n      </div>\r\n      <nav class=\"main-nav\">\r\n        <router-link to=\"/\">Dashboard</router-link>\r\n\r\n        <div class=\"dropdown\">\r\n          <a class=\"dropdown-toggle\">Accounts</a>\r\n          <div class=\"dropdown-menu\">\r\n            <router-link to=\"/accounts/companies\">Companies</router-link>\r\n            <router-link to=\"/accounts/user-roles\">User Roles</router-link>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"dropdown\">\r\n          <a class=\"dropdown-toggle\">Core</a>\r\n          <div class=\"dropdown-menu\">\r\n            <router-link to=\"/core/bills\">Bills</router-link>\r\n            <router-link to=\"/core/chart-of-accounts\">Chart of Accounts</router-link>\r\n            <router-link to=\"/core/customers\">Customers</router-link>\r\n            <router-link to=\"/core/general-ledger\">General Ledger</router-link>\r\n            <router-link to=\"/core/invoices\">Invoices</router-link>\r\n            <router-link to=\"/core/vendors\">Vendors</router-link>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"dropdown\">\r\n          <a class=\"dropdown-toggle\">Payroll</a>\r\n          <div class=\"dropdown-menu\">\r\n            <router-link to=\"/payroll/employees\">Employees</router-link>\r\n            <router-link to=\"/payroll/payrolls\">Payrolls</router-link>\r\n            <router-link to=\"/payroll/paystubs\">Paystubs</router-link>\r\n            <router-link to=\"/payroll/taxes\">Taxes</router-link>\r\n            <router-link to=\"/payroll/deductions\">Deductions</router-link>\r\n            <router-link to=\"/payroll/benefits\">Benefits</router-link>\r\n          </div>\r\n        </div>\r\n\r\n        <router-link to=\"/subscriptions\">Subscriptions</router-link>\r\n        <router-link to=\"/reports\">Reports</router-link>\r\n\r\n      </nav>\r\n      <div class=\"user-actions\">\r\n        <CompanySelector v-if=\"isLoggedIn\" />\r\n        <button v-if=\"!isLoggedIn\" @click=\"login\">Login</button>\r\n        <button v-if=\"isLoggedIn\" @click=\"logout\">Logout</button>\r\n      </div>\r\n    </header>\r\n    <main class=\"app-content\">\r\n      <router-view/>\r\n    </main>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapActions } from 'vuex';\r\nimport CompanySelector from './components/CompanySelector.vue';\r\n\r\nexport default {\r\n  name: 'App',\r\n  components: {\r\n    CompanySelector,\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      isLoggedIn: state => !!state.user.token,\r\n    }),\r\n  },\r\n  methods: {\r\n    ...mapActions({\r\n      logoutAction: 'user/logout',\r\n    }),\r\n    login() {\r\n      this.$router.push({ name: 'Login' });\r\n    },\r\n    logout() {\r\n      this.logoutAction().then(() => {\r\n        this.$router.push({ name: 'Login' });\r\n      });\r\n    },\r\n  },\r\n};\r\n</scrip