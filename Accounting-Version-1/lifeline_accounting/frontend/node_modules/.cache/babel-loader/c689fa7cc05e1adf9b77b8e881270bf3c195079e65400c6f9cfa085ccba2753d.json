{"ast":null,"code":"import { mapActions, mapState } from 'vuex';\nexport default {\n  name: 'Login',\n  data() {\n    return {\n      username: '',\n      password: ''\n    };\n  },\n  computed: {\n    ...mapState('user', ['loading', 'error'])\n  },\n  methods: {\n    ...mapActions('user', ['login']),\n    handleLogin() {\n      this.login({\n        username: this.username,\n        password: this.password\n      }).then(() => {\n        if (!this.error) {\n          this.$router.push({\n            name: 'Dashboard'\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["mapActions","mapState","name","data","username","password","computed","methods","handleLogin","login","then","error","$router","push"],"sources":["C:\\Users\\ds3297l\\Desktop\\Accountint Software\\Design 2\\lifeline_accounting\\frontend\\src\\components\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-view\">\r\n    <div class=\"login-panel\">\r\n      <div class=\"login-header\">\r\n        <h2>Welcome Back</h2>\r\n        <p>Please enter your credentials to log in.</p>\r\n      </div>\r\n      <form @submit.prevent=\"handleLogin\">\r\n        <div class=\"form-group\">\r\n          <label for=\"username\">Username</label>\r\n          <input type=\"text\" id=\"username\" v-model=\"username\" required placeholder=\"Enter your username\">\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"password\">Password</label>\r\n          <input type=\"password\" id=\"password\" v-model=\"password\" required placeholder=\"Enter your password\">\r\n        </div>\r\n        <button type=\"submit\" class=\"login-button\" :disabled=\"loading\">\r\n          <span v-if=\"loading\">Logging in...</span>\r\n          <span v-else>Login</span>\r\n        </button>\r\n        <p v-if=\"error\" class=\"error-message\">{{ error }}</p>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions, mapState } from 'vuex';\r\n\r\nexport default {\r\n  name: 'Login',\r\n  data() {\r\n    return {\r\n      username: '',\r\n      password: '',\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState('user', ['loading', 'error']),\r\n  },\r\n  methods: {\r\n    ...mapActions('user', ['login']),\r\n    handleLogin() {\r\n      this.login({ username: this.username, password: this.password })\r\n        .then(() => {\r\n          if (!this.error) {\r\n            this.$router.push({ name: 'Dashboard' });\r\n          }\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login-view {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  height: 100%;\r\n  background-color: #f4f7f6;\r\n}\r\n\r\n.login-panel {\r\n  width: 100%;\r\n  max-width: 420px;\r\n  padding: 2.5rem;\r\n  background: #ffffff;\r\n  border-radius: 10px;\r\n  box-shadow: 0 10px 25px rgba(0, 0, 0, 0.1);\r\n  text-align: center;\r\n}\r\n\r\n.login-header h2 {\r\n  font-size: 2rem;\r\n  font-weight: 600;\r\n  color: #2c3e50;\r\n  margin-bottom: 0.5rem;\r\n}\r\n\r\n.login-header p {\r\n  color: #7f8c8d;\r\n  margin-bottom: 2rem;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 1.5rem;\r\n  text-align: left;\r\n}\r\n\r\n.form-group label {\r\n  display: block;\r\n  margin-bottom: 0.5rem;\r\n  font-weight: 500;\r\n  color: #34495e;\r\n}\r\n\r\n.form-group input {\r\n  width: 100%;\r\n  padding: 0.75rem 1rem;\r\n  border: 1px solid #dce4e8;\r\n  border-radius: 5px;\r\n  font-size: 1rem;\r\n  transition: border-color 0.3s ease;\r\n}\r\n\r\n.form-group input:focus {\r\n  outline: none;\r\n  border-color: #42b983;\r\n}\r\n\r\n.login-button {\r\n  width: 100%;\r\n  padding: 0.85rem;\r\n  border: none;\r\n  border-radius: 5px;\r\n  background-color: #42b983;\r\n  color: white;\r\n  font-size: 1.1rem;\r\n  font-weight: 500;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.login-button:hover {\r\n  background-color: #36a476;\r\n}\r\n\r\n.login-button:disabled {\r\n  background-color: #a4d4be;\r\n  cursor: not-allowed;\r\n}\r\n\r\n.error-message {\r\n  color: #e74c3c;\r\n  margin-top: 1rem;\r\n  font-weight: 500;\r\n}\r\n</style>\r\n"],"mappings":"AA2BA,SAASA,UAAU,EAAEC,QAAO,QAAS,MAAM;AAE3C,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGL,QAAQ,CAAC,MAAM,EAAE,CAAC,SAAS,EAAE,OAAO,CAAC;EAC1C,CAAC;EACDM,OAAO,EAAE;IACP,GAAGP,UAAU,CAAC,MAAM,EAAE,CAAC,OAAO,CAAC,CAAC;IAChCQ,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACC,KAAK,CAAC;QAAEL,QAAQ,EAAE,IAAI,CAACA,QAAQ;QAAEC,QAAQ,EAAE,IAAI,CAACA;MAAS,CAAC,EAC5DK,IAAI,CAAC,MAAM;QACV,IAAI,CAAC,IAAI,CAACC,KAAK,EAAE;UACf,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;YAAEX,IAAI,EAAE;UAAY,CAAC,CAAC;QAC1C;MACF,CAAC,CAAC;IACN;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}