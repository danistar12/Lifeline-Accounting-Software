    return setTimeout(resolve, 1500);\n              });\n            case 3:\n              alert('Password changed successfully!');\n              _this.passwordForm = {\n                current_password: '',\n                new_password: '',\n                confirm_password: ''\n              };\n              _context.n = 5;\n              break;\n            case 4:\n              _context.p = 4;\n              _t = _context.v;\n              console.error('Failed to change password:', _t);\n              alert('Failed to change password. Please try again.');\n            case 5:\n              _context.p = 5;\n              _this.changingPassword = false;\n              return _context.f(5);\n            case 6:\n              return _context.a(2);\n          }\n        }, _callee, null, [[2, 4, 5, 6]]);\n      }))();\n    },\n    savePreferences: function savePreferences() {\n      var _this2 = this;\n      return _asyncToGenerator(/*#__PURE__*/_regenerator().m(function _callee2() {\n        var _t2;\n        return _regenerator().w(function (_context2) {\n          while (1) switch (_context2.n) {\n            case 0:\n              _this2.savingPreferences = true;\n              _context2.p = 1;\n              // This would connect to a preferences API endpoint\n              console.log('Saving preferences...', _this2.preferences);\n\n              // Simulate API call\n              _context2.n = 2;\n              return new Promise(function (resolve) {\n                return setTimeout(resolve, 1000);\n              });\n            case 2:\n              alert('Preferences saved successfully!');\n              _context2.n = 4;\n              break;\n            case 3:\n              _context2.p = 3;\n              _t2 = _context2.v;\n              console.error('Failed to save preferences:', _t2);\n              alert('Failed to save preferences. Please try again.');\n            case 4:\n              _context2.p = 4;\n              _this2.savingPr